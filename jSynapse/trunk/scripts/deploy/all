export CURRENT_PATH=`pwd`
cd $SYNAPSE_HOME/jSynapse/trunk/bin

echo "--------------------------------------"
echo "|      generate jSynapse library     |"
echo "--------------------------------------" 
echo ""
echo "jar -cfv $SYNAPSE_HOME/jSynapse/trunk/dist/lib/jSynapse.jar $SYNAPSE_HOME/jSynapse/trunk/bin/*"
sleep 2
jar -cfv $SYNAPSE_HOME/jSynapse/trunk/dist/lib/jSynapse.jar *

sleep 1
echo ""
echo "--------------------------------------"
echo "|       deploy jSynapse library      |"
echo "--------------------------------------"
echo ""
echo "cp $SYNAPSE_HOME/jSynapse/trunk/dist/lib/jSynapse.jar $SYNAPSE_HOME/myTransport/lib/"
cp $SYNAPSE_HOME/jSynapse/trunk/dist/lib/jSynapse.jar $SYNAPSE_HOME/myTransport/lib/
sleep 1
echo "cp $SYNAPSE_HOME/jSynapse/trunk/dist/lib/jSynapse.jar $SYNAPSE_HOME/tracker/lib/"
cp $SYNAPSE_HOME/jSynapse/trunk/dist/lib/jSynapse.jar $SYNAPSE_HOME/tracker/lib/

sleep 1
echo ""
echo "done."

cd $CURRENT_PATH
